	<script type="text/javascript">
		
        $("#sure_creLM").hide();
        $("#exit_creLM").hide();
        $("#notName").hide();
        $(".fBN").hide();
        $(".fBC").hide();
        $(".fBC_b").hide();
        $("#sure_findByName").attr("disabled",true);

        function addClickHandler(content,marker,tit){
            marker.addEventListener("click", function(e){
                var p = e.target;
                var point = new BMap.Point(p.getPosition().lng, p.getPosition().lat);
                var searchInfoWindow = null;
                searchInfoWindow = new BMapLib.SearchInfoWindow(map, content, {
                    title  : tit,      //标题
                    width  : 290,             //宽度
                    height : 105,              //高度
                    panel  : "panel",         //检索结果面板
                    enableAutoPan : true,     //自动平移
                    searchTypes :[]
                });
                searchInfoWindow.open(point);
            });
        }

        function showLandMark(ml){
            for(var i=0;i<ml.length;i++){
                var marker = new BMap.Marker(new BMap.Point(ml[i].longitude,ml[i].latitude));  // 创建标注
                var content ='<div style="margin:0;line-height:20px;padding:2px;">' +
                        '<style="float:right;zoom:1;overflow:hidden;width:100px;height:100px;margin-left:3px;"/>'+
                        '坐标:('+ml[i].longitude+','+ml[i].latitude+')<br/>'+'内容:'+ml[i].content+'<br/>'+'分类:'+ml[i].category+'<br/>'+'<button class="btn-default" id="'+ml[i].name+'" onclick="sureDelete(this)">删除'+'</button>'+'  '+'<button class="btn-default" id="'+ml[i].name+'"onclick="Edit(this)">修改'+'</button>'+'</div>';
                map.addOverlay(marker); 
                var label = new BMap.Label(ml[i].name,{offset:new BMap.Size(20,-10)});
                marker.setLabel(label);
                label.setStyle({
                     color : "black",
                     fontSize : "12px",
                     height : "20px",
                     backgroundColor :"0.05",
                     border:"0",
                     textAlign:"center",
                     lineHeight : "20px",
                     fontFamily:"微软雅黑"
                });
                addClickHandler(content,marker,ml[i].name);
            }
        }

        var json_ml;
        $.ajax({
                type: "post",
                async: false,
                url: "/background/back_mainPage",
                data : {
                    type : "3",
                },
             success: function(data){
                        json_ml=data;                        
                    }   
        });
        showLandMark(json_ml);




        //显示所有地标
        $("#findAll").click(function(){
            var allOverlay = map.getOverlays();
            for (var i = 0; i < allOverlay.length; i++){
                map.removeOverlay(allOverlay[i]);
            }
            showLandMark(json_ml);
        });

        //按名称查找
        $("#findByName").click(function(){
            $("#findByName").attr("disabled","true");
            $(".fBN").show("500");
            $("#fBN_Name").val("");
        });
        $("#fBN_Name").keyup(function(){
            var fBN_Name = $("#fBN_Name").val();
            if(fBN_Name == null || fBN_Name == undefined || fBN_Name == ""){
                $("#sure_findByName").attr("disabled","true");
            }else{
                $("#sure_findByName").attr("disabled",false);
            }
        });
        $("#sure_findByName").click(function(){
            var fBN_Name = $("#fBN_Name").val();
            if(!(fBN_Name == null || fBN_Name == undefined || fBN_Name == "")){
                $.ajax({
                    type: "post",
                    url: "/background/back_mainPage",
                    data : {
                        type : "2",
                        name : fBN_Name,
                    },
                 success: function(data){
                            var allOverlay = map.getOverlays();
                            for (var i = 0; i < allOverlay.length; i++){
                                map.removeOverlay(allOverlay[i]);
                            }
                            if(data.length != 0){
                                showLandMark(data);
                            }else{
                                $('#notFindLM').modal('show');
                            } 
                        }  
                });
            }else{
                $("#sure_findByName").attr("disabled","true");
            }
        });
        $("#not_findByName").click(function(){
            $(".fBN").hide("500");
            $("#findByName").attr("disabled",false);
        });

        //按分类查找
        $("#findByCate").click(function(){
            $("#findByCate").attr("disabled","true");
            $(".find_ca0").show("50");
            $(".fBC_b").show("50");
            $(".find_cateA").mouseleave();

        });
        $(".find_cateA").mouseleave(function(){
            var opA = $("#find_getCaInfo0").val();
            $(".find_cateB").hide("50");
            $(".find_cateC").hide("50");
            if(opA == "学校设施"){
                $(".find_ca0_0").show("50");
            }else if(opA == "生活设施"){
                $(".find_ca0_1").show("50");
            }
        });
        $(".find_cateB").mouseleave (function(){
